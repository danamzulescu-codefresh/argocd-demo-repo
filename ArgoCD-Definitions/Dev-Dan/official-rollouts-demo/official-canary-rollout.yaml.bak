apiVersion: argoproj.io/v1alpha1
kind: Application
metadata:
  name: official-canary-rollout
  namespace: dan-a-eks
spec:
  destination:
    namespace: dan-a-eks
    server: https://kubernetes.default.svc
  project: dan-a-eks
  source:
    path: dev/official-rollouts-demo/canary/
    repoURL: https://github.com/danamzulescu-codefresh/k8s-apps/
    targetRevision: HEAD
  syncPolicy:
    automated:
      prune: true